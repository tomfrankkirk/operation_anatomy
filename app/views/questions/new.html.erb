<h1> New Question </h1>

<%= form_for :question, url: questions_path do |form| %>
  <p>
    <%= form.label :body %><br>
    <%= form.text_field :body %>
  </p>
 
  <p>
    <%= form.label :level %><br>
    <%= form.text_area :level %>
  </p>
 
  <p>
    <%= form.submit %>
  </p>
<% end %>

<%= link_to 'Back', questions_path %>