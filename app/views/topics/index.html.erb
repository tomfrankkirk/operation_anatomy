<h1>Hello <%= current_user.email %> </h1>
 
<table>
  <tr>
    <th>Name</th>
    <th>Number of levels</th>
  </tr>
 
  <% @topics.each do |t| %>
    <tr>
      <td><%= t.name %></td>
      <td><%= t.numberOfLevels %></td>
      <td><%= link_to 'Show', topic_path(t) %></td>
    </tr>
  <% end %>
</table>